{
  "name": "website-fish-keeper",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "pw:install": "playwright install --with-deps",
    "test": "playwright test",
    "test:e2e": "playwright test --config=playwright.e2e.config.ts",
    "test:consent": "playwright test tests/consent-ads.spec.ts --project=chromium",
    "serve:static": "npx http-server -p 8080 -c-1 .",
    "test:headed": "npx playwright test --headed",
    "test:debug": "npx playwright test --debug",
    "test:report": "npx playwright show-report",
    "audit:amazon:img": "node tests/verify-amazon-image-links.cjs",
    "build:stands": "node scripts/build-gear-stands.mjs",
    "audit:filters": "node scripts/audit-filters-catalog.mjs",
    "build": "node scripts/build-prod.mjs",
    "footer:verify": "node scripts/footer-lock/verify.mjs",
    "footer:lint": "node scripts/footer-lock/lint.mjs",
    "footer:lock:update": "node scripts/footer-lock/update-manifest.mjs",
    "prepare": "node scripts/footer-lock/setup-hooks.mjs"
  },
  "devDependencies": {
    "@playwright/test": "^1.42.0"
  }
}
